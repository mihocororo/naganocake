  <body class="d-flex h-100 text-center text-black bg-light">

<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
  <header class="mb-auto">
    <div>


      <nav class="nav nav-masthead justify-content-center float-md-end">
        <!--<a class="nav-link active" aria-current="page" href="#">Home</a>-->
        <a class="nav-link px-2" href="/admin/items">商品一覧</a>
        <a class="nav-link px-2" href="/admin/customers">会員一覧</a>
        <a class="nav-link px-2" href="/admin">注文履歴一覧</a>
        <a class="nav-link px-2" href="/admin/genres">ジャンル一覧</a>
        <a class="nav-link px-2" href="/admin/sign_out">ログアウト</a>
        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
        </form>

        <li class="nav-item">
          <%= link_to 'admin', class: "no-underline" do %>
          <!--<i class="fas fa-home"></i>-->
          <h4><i class="fa-solid fa-cart-shopping"></i></h4>
          <% end %>
      </nav>
    </div>
  </header>

  <main class="px-3">

<%= form_with model: @item, url: admin_item_path, method: :patch, remote: true do |f| %>
  <a>商品画像</a>
  <a>商品名</a><%= f.text_field :name,autocomplete: "name" %></br>
  <a>商品説明</a><%= f.text_area :introduction %></br>
  <a>ジャンル</a><%= f.collection_select :genre_id, Genre.all, :id, :name, include_blank: "--選択して下さい--" %></br>
<a>税抜価格</a><%= f.text_area :price %></br>

<tr>
<th><%= f.label :販売ステータス %></th>
<td><%= f.radio_button :is_active, true,checked: true %>
<%= f.label :is_active, "販売中" %>
<%= f.radio_button :is_active, false %>
<%= f.label :is_active, "販売停止中" %></td></br>
</tr>

<th>
  <%= f.submit '変更を保存' %></th>
  <% end %>
  </main>

