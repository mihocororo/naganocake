<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
  <% if request.fullpath.include? "admin" %>
  <header class="mb-auto">
    <div>
      <nav class="nav nav-masthead justify-content-center float-md-end">
        <!--<a class="nav-link active" aria-current="page" href="#">Home</a>-->
        <% if admin_signed_in? %>
        <% else %>
        <a class="nav-link" href="/about">About</a>
        <% end %>
        <a class="nav-link px-2" href="/admin/items">商品一覧</a>
        <% if admin_signed_in? %>
        <a class="nav-link px-2" href="/admin/customers">会員一覧</a>
        <a class="nav-link px-2" href="/admin">注文履歴一覧</a>
        <a class="nav-link px-2" href="/admin/genres">ジャンル一覧</a>
        <% else %>
        <a class="nav-link" href="/customers/sign_up">新規登録</a>
        <% end %>
        <% if admin_signed_in? %>
        <a class="nav-link px-2" href="/admin/sign_out">ログアウト</a>
        <% else %>
        <a class="nav-link px-2" href="/admin/sign_in">ログイン</a>
        <% end %>
        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
        </form>

        <li class="nav-item">
          <%= link_to 'admin', class: "no-underline" do %>
          <!--<i class="fas fa-home"></i>-->
          <h4><i class="fa-solid fa-cart-shopping"></i></h4>
          <% end %>
      </nav>


    </div>

  </header>

  <% else %>
  <header class="mb-auto">
    <div>
<nav class="nav nav-masthead justify-content-left ">
<div class= "logo">
        <a class="nav-link px-2 text-stroke-color-white text-dark" href="/" >ながのCAKE</a></div>
        </nav>
      <nav class="nav nav-masthead justify-content-center float-md-end">

        <% if customer_signed_in? %>
        <a class="nav-link px-2" href="/customers/my_page">マイページ</a>
        <% else %>
         <a class="nav-link" href="/about">About</a>
        <% end %>
        <a class="nav-link" href="/items">商品一覧</a>
        <a class="nav-link" href="/customers/sign_up">新規登録</a>
        <% if customer_signed_in? %>
        <a class="nav-link px-2" href="/customers/sign_out">ログアウト</a>
        <% else %>
        <a class="nav-link" href="/customers/sign_in">ログイン</a>
        <% end %>
        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
        </form>
        <li class="nav-item">
          <%= link_to '/cart_items', class: "no-underline" do %>
          <h4><i class="fa-solid fa-cart-shopping"></i></h4>
          <% end %>
      </nav>
    </div>
  </header>
  <% end %>
    <%= yield %>
  </body>
</html>
